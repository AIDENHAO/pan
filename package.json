{
  "name": "react-typescript",
  "version": "1.0.0",
  "type": "module",
  "description": "React TypeScript on CloudStudio, using Vite bundler",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "server": "ts-node --esm src/server/server.ts",
    "server:old": "node src/server/server.js",
    "server:dev": "NODE_ENV=development ts-node --esm src/server/server.ts",
    "db:test": "ts-node --esm src/database/test.ts basic",
    "db:test:full": "ts-node --esm src/database/test.ts full",
    "db:cleanup": "ts-node --esm src/database/test.ts cleanup",
    "db:example": "ts-node --esm src/database/examples/usage.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@types/axios": "^0.14.4",
    "@types/express-validator": "^3.0.2",
    "antd": "^5.26.6",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "mysql2": "^3.6.5",
    "react-router-dom": "^6.30.1"
  }
}
